{
  "name": "labour_tracker_frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview --port 5173",
    "generate-client": "openapi-ts",
    "typecheck": "tsc --noEmit",
    "lint": "npm run eslint && npm run stylelint",
    "style:fix": "npm run stylelint:fix && npm run prettier:write && npm run eslint:fix",
    "eslint": "eslint src",
    "eslint:fix": "eslint src --fix",
    "stylelint": "stylelint '**/*.css' --cache",
    "stylelint:fix": "stylelint '**/*.css' --fix",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "test": "npm run prettier:check && npm run lint && npm run typecheck"
  },
  "dependencies": {
    "@hey-api/openapi-ts": "^0.60.1",
    "@mantine/charts": "^7.17.0",
    "@mantine/core": "^7.15.2",
    "@mantine/dates": "^7.16.3",
    "@mantine/form": "^7.15.2",
    "@mantine/hooks": "^7.15.2",
    "@mantine/notifications": "^7.15.3",
    "@tabler/icons-react": "^3.26.0",
    "@tanstack/react-query": "^5.59.15",
    "@types/node": "^20.16.13",
    "axios": "^1.7.9",
    "oidc-client-ts": "^3.1.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-loader-spinner": "^6.1.6",
    "react-oidc-context": "^3.2.0",
    "react-router-dom": "^6.27.0",
    "recharts": "^2.15.1",
    "scule": "^1.3.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.19.0",
    "eslint-config-mantine": "^4.0.3",
    "eslint-plugin-react": "^7.37.4",
    "postcss": "^8.4.49",
    "postcss-preset-mantine": "^1.17.0",
    "prettier": "^3.4.2",
    "stylelint": "^16.14.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vite-plugin-eslint": "^1.8.1"
  }
}
